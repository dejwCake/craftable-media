{
    "name": "dejwcake/craftable-media",
    "description": "Associate files with Eloquent models",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "media",
        "medialibrary",
        "images",
        "cms",
        "laravel"
    ],
    "authors": [
        {
            "name": "David Běhal",
            "email": "david.behal@dejw.sk",
            "homepage": "https://www.dejw.sk",
            "role": "Developer"
        },
        {
            "name": "Pavol Perdík",
            "email": "pavol.perdik@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        },
        {
            "name": "Marek Kratochvíla",
            "email": "marek.kratochvila@brackets.sk",
            "homepage": "https://www.brackets.sk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.41",
        "spatie/laravel-medialibrary": "^11.12"
    },
    "require-dev": {
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^9.9",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Brackets\\Media\\": "src/"
        },
        "classmap": []
    },
    "autoload-dev": {
        "psr-4": {
            "Brackets\\Media\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Brackets\\Media\\MediaServiceProvider"
            ]
        }
    },
    "branch-alias": {
        "dev-main": "1.0-dev"
    }
}
